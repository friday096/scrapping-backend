{
    "version": 2,
    "builds": [
      {
        "src": "bin/www",
        "use": "@vercel/node",
        "config": {
          "env": {
            "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
          }
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "bin/www"
      }
    ]
  }
  